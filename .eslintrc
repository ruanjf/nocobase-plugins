{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "plugin:promise/recommended",
    // "plugin:import/recommended",
    "plugin:prettier/recommended"
  ],
  "env": {
    "node": true
  },
  "globals": {
    "sleep": true,
    "prettyFormat": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  }
}
